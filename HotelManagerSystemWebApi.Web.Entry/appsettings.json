{
  "DynamicApiControllerSettings": {
    "ModelToQuery": true,
    "KeepVerb": true,
    "KeepName": true,
    "VerbToHttpMethods": [
      [ "Insert", "POST" ],
      [ "Add", "POST" ],
      [ "Del", "DELETE" ],
      [ "Delete", "DELETE" ],
      [ "Upd", "POST" ],
      [ "Update", "POST" ],
      [ "Select", "GET" ]
    ],
    "DefaultHttpMethod": "GET",
    "LowercaseRoute":false
  },
    "Logging": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.EntityFrameworkCore": "Information"
      }
    },
    "AllowedHosts": "*"
  }